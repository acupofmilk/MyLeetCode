class Solution:
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        for num in nums:
            index_now = nums.index(num)
            for after_index in range(index_now + 1,len(nums)):
                if num + nums[after_index] == target:
                    return (nums.index(num),after_index)
#通过array.index(value)获得序列号；                    
